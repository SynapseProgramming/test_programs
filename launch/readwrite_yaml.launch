<?xml version="1.0" encoding="UTF-8"?>
<launch>

<!--Placing the rosparam tag in the global namespace means that there are no namespace slashes-->
<rosparam file="$(find test_programs)/param/readwrite_test.yaml" command="load"/>


<node pkg="test_programs" type="readwrite_yaml_node" name="yaml_test" output="screen" launch-prefix="gnome-terminal --command"/>
<!--Placing the rosparam tag in a node means that there are namespace slashes-->
</launch>
